{
    "name": "iaxel/shared",
    "description": "Private library for Laravel project.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sukhrob (iAxel) Karshiev",
            "email": "iaxel0297@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "iAxel\\Shared\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "iAxel\\Shared\\Providers\\SharedServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
